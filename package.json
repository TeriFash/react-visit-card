{
  "name": "react-static-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "latest",
    "history": "^5.1.0",
    "node-sass": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "latest",
    "react-spring": "latest",
    "vanilla-tilt": "latest",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.{js,md,ts,json}\" *.{js,md,ts,json}",
    "prod": "react-scripts build && yarn global add serve && serve -s build",
    "ftp-deploy": "node deploy.js",
    "deploy": "yarn ftp-deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,md,ts,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ftp-deploy": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "lint-staged": "12.1.2",
    "prettier": "^2.5.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/plugin-proposal-function-bind": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-imports": "2.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-react-hooks": "2.5.1",
    "eslint-plugin-testing-library": "4.12.4",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-react": "^7.27.1"
  }
}
